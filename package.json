{
  "name": "@jesses/exif-parser",
  "description": "A dependency-free library to extract Exif metadata from images.",
  "version": "1.0.0",
  "author": [
    "Jesse Stuart <hi@jessestuart.com>",
    "Bruno Windels <bruno.windels@gmail.com>"
  ],
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-typescript": "7.3.3",
    "@babel/runtime": "7.4.3",
    "@semantic-release/git": "7.0.8",
    "@types/jest": "24.0.11",
    "@types/lodash": "4.14.123",
    "@types/node": "11.13.7",
    "@typescript-eslint/eslint-plugin": "1.7.0",
    "@typescript-eslint/parser": "1.7.0",
    "babel-preset-minify": "0.5.0",
    "codecov": "3.3.0",
    "core-js": "3.0.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-prettier": "3.0.1",
    "jest": "24.7.1",
    "lodash": "4.17.11",
    "prettier": "1.17.0",
    "semantic-release": "15.13.3",
    "ts-jest": "24.0.2",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.4.5"
  },
  "keywords": [
    "exif",
    "gps",
    "image",
    "jpeg",
    "jpg",
    "photography",
    "tiff",
    "typescript"
  ],
  "main": "dist/index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jessestuart/exif-parser.git"
  },
  "scripts": {
    "build": "babel --extensions '.ts' cmd/* lib/* index.ts --out-dir dist/",
    "lint": "eslint ./cmd/**/*.ts ./lib/**/*.ts ./test/**/*.ts",
    "test": "jest"
  }
}
