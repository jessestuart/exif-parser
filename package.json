{
  "name": "@jesses/exif-parser",
  "description": "A dependency-free library to extract Exif metadata from images.",
  "version": "1.1.21",
  "author": [
    "Jesse Stuart <hi@jessestuart.com>",
    "Bruno Windels <bruno.windels@gmail.com>"
  ],
  "dependencies": {
    "luxon": "1.21.1"
  },
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.7.2",
    "@babel/plugin-proposal-class-properties": "7.7.0",
    "@babel/preset-env": "7.7.1",
    "@babel/preset-typescript": "7.7.2",
    "@semantic-release/git": "7.0.18",
    "@types/jest": "24.0.23",
    "@types/lodash": "4.14.148",
    "@types/luxon": "1.21.0",
    "@types/node": "12.12.8",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "babel-preset-minify": "0.5.1",
    "codecov": "3.6.1",
    "core-js": "3.4.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "jest": "24.9.0",
    "lodash": "4.17.15",
    "prettier": "1.19.1",
    "rimraf": "2.7.1",
    "semantic-release": "15.13.30",
    "ts-jest": "24.1.0",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.7.2"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "exif",
    "gps",
    "image",
    "jpeg",
    "jpg",
    "photography",
    "tiff",
    "typescript"
  ],
  "main": "exif-parser.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jessestuart/exif-parser.git"
  },
  "scripts": {
    "build": "babel --extensions '.ts' src/ -d dist/",
    "clean": "rimraf dist/index.js dist/**/*.js",
    "lint": "eslint ./src/**/*.ts ./test/**/*.ts",
    "prepack": "yarn build",
    "test": "jest"
  }
}
