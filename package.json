{
  "name": "@jesses/exif-parser",
  "description": "A dependency-free library to extract Exif metadata from images.",
  "version": "1.1.6",
  "author": [
    "Jesse Stuart <hi@jessestuart.com>",
    "Bruno Windels <bruno.windels@gmail.com>"
  ],
  "dependencies": {
    "luxon": "1.16.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-typescript": "7.3.3",
    "@semantic-release/git": "7.0.14",
    "@types/jest": "24.0.15",
    "@types/lodash": "4.14.135",
    "@types/luxon": "1.15.1",
    "@types/node": "12.0.10",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "babel-preset-minify": "0.5.0",
    "codecov": "3.5.0",
    "core-js": "3.1.4",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-prettier": "3.1.0",
    "jest": "24.8.0",
    "lodash": "4.17.11",
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.18",
    "ts-jest": "24.0.2",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.5.2"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "exif",
    "gps",
    "image",
    "jpeg",
    "jpg",
    "photography",
    "tiff",
    "typescript"
  ],
  "main": "exif-parser.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jessestuart/exif-parser.git"
  },
  "scripts": {
    "build": "babel --extensions '.ts' src/ -d dist/",
    "clean": "rimraf dist/index.js dist/**/*.js",
    "lint": "eslint ./src/**/*.ts ./test/**/*.ts",
    "prepack": "yarn build",
    "test": "jest"
  }
}
